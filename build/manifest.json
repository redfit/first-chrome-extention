{
    "name": "My Fifth Extension",
    "version": "0.1",
    "manifest_version": 2,

    "description": "dotinstallの背景色を変える！",

    "permissions": [
        "tabs", "http://*/*"
    ],

    "background": {
        "scripts": ["background.js"]
    },

    "page_action": {
        "default_icon": "icon19.png",
        "default_title": "My Fifth Extension"
    }
}
